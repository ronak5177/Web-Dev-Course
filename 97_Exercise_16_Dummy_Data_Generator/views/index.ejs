<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Employee Data Generator</title>
  </head>
  <body>
    <h2>Employee Data Generator</h2>
    <div class="container">
      <button type="button">Generate Now</button>
    </div>
    <script>
      document
        .querySelector(".container > button")
        .addEventListener("click", async () => {
          let a = await fetch("/generate");
          let r = await a.json();
          console.log(r);
        });
    </script>
  </body>
</html>
